{
    "name": "harbor",
    "version": "1.3.0",
    "description": "Harbor UI with Clarity",
    "angular-cli": {},
    "scripts": {
        "start": "ng serve --ssl 1 --ssl-key ssl/server.key --ssl-cert ssl/server.crt --host 0.0.0.0 --proxy-config proxy.config.json",
        "lint": "tslint \"src/**/*.ts\"",
        "lint:lib": "tslint \"lib/**/*.ts\"",
